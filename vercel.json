{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "outputDirectory": "dist/client",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/_serverFn/:path*",
      "destination": "/_serverFn/:path*"
    }
  ]
}
